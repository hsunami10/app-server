{
	// Transpile order ends here. Bottom -> top.
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current"
				}
			}
		],
		"@babel/preset-typescript"
	],
	// Transpile order starts here. Top -> bottom.
	// Plugins are always applied top to bottom.
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./src"],
				"alias": {
					"underscore": "lodash"
				}
			}
		]
	]
}
